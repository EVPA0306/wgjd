<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate//Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.eap.wgjd.chaptereleven.Ticket">
        <id name="ticketId" type="long" column="ID" />
        <property name="faceValue" type="java.math.BigDecimal" column="FACE_VALUE" not-null="false"/>
        <property name="discountRate" type="java.math.BigDecimal" column="DISCOUNT_RATE" not-null="false"/>
    </class>
</hibernate-mapping>